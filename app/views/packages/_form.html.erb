<%= simple_form_for(@package) do |f| %>
  <%= f.input :sip_path %>
  <%= f.association :status %>
  <%= f.association :batch %>
  <%= f.input :aip_identifier %>
  <%= f.input :dip_identifier %>
  <%= f.input :oral_history, :as => :radio_buttons %>
  <%= f.input :dark_archive, :as => :radio_buttons %>
  <%= f.input :approved, :as => radio_buttons %>
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
